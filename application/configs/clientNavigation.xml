<?xml version="1.0" encoding="UTF-8"?>
<configdata>
	<nav>
		<home>
			<label>Domů</label>
			<route>home</route>
		</home>
		<client>
			<label>Klienti</label>
			<route>clientList</route>
			<resource>client</resource>
			<privilege>list</privilege>
			<pages>
				<choose>
					<label>Vyber jiného klienta</label>
					<route>clientList</route>
					<resource>client</resource>
					<privilege>list</privilege>
				</choose>
				<search>
					<label>Hledej</label>
					<route>search</route>
					<resource>search</resource>
				</search>
				<new>
					<label>Založ nového klienta</label>
					<route>clientNew</route>
					<resource>client</resource>
					<privilege>new</privilege>
				</new>
			</pages>
		</client>
		<subsidiary>
			<label>Pobočky</label>
			<route>subsidiaryList</route>
			<resource>subsidiary</resource>
			<privilege>list</privilege>
			<clientId>clientId</clientId>
		</subsidiary>
		<mistakelist>
			<label>Neshody</label>
			<resource>audit:mistake</resource>
			<privilege>index</privilege>
			<route>audit-mistakes-index</route>
			<clientId>clientId</clientId>
		</mistakelist>
		<audits>
			<label>Audity</label>
			<resource>audit:audit</resource>
			<privilege>list</privilege>
			<route>audit-list</route>
			<clientId>clientId</clientId>
		</audits>
		<print>
			<label>Záznamy (k tisku)</label>
			<route>printIndex</route>
			<resource>print</resource>
			<privilege>index</privilege>
			<clientId>clientId</clientId>
			<pages>
				<diary>
					<label>Historie BD</label>
					<route>printDiary</route>
					<resource>print</resource>
					<privilege>diary</privilege>
					<clientId>clientId</clientId>
				</diary>
			</pages>
		</print>
		<workplace>
			<label>Pracoviště</label>
			<route>workplaceList</route>
			<resource>workplace</resource>
			<privilege>list</privilege>
			<clientId>clientId</clientId>
			<subsidiaryId>subsidiaryId</subsidiaryId>
			<filter>filter</filter>
			<pages>
				<list>
					<label>Databáze pracovišť</label>
					<route>workplaceList</route>
					<resource>workplace</resource>
					<privilege>list</privilege>
					<clientId>clientId</clientId>
					<subsidiaryId>subsidiaryId</subsidiaryId>
					<filter>filter</filter>
				</list>
			</pages>
		</workplace>
		<position>
			<label>Databáze pracovních pozic</label>
			<route>positionList</route>
			<resource>position</resource>
			<privilege>list</privilege>
			<clientId>clientId</clientId>
			<subsidiaryId>subsidiaryId</subsidiaryId>
			<filter>filter</filter>
		</position>
