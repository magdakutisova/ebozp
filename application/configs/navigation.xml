<?xml version="1.0" encoding="UTF-8"?>
<configdata>
	<nav>
		<home>
			<label>Domů</label>
			<route>home</route>
		</home>
		<client>
			<label>Klienti</label>
			<route>clientList</route>
			<resource>client</resource>
			<privilege>list</privilege>
			<pages>
				<choose>
					<label>Vyber ze seznamu</label>
					<route>clientList</route>
					<resource>client</resource>
					<privilege>list</privilege>
				</choose>
				<search>
					<label>Hledej</label>
					<route>search</route>
					<resource>search</resource>
				</search>
				<new>
					<label>Založ nového klienta</label>
					<route>clientNew</route>
					<resource>client</resource>
					<privilege>new</privilege>
				</new>
				<audit><label>Nový audit</label>
					<resource>audit:audit</resource>
					<privilege>create</privilege>
					<uri>/audit/audit/index</uri>
					</audit>
				</pages>
		</client>
		<clientArchivelist>
			<label>Archiv klientů</label>
			<route>clientArchivelist</route>
			<resource>client</resource>
			<privilege>archivelist</privilege>
		</clientArchivelist>
		<userAdmin>
			<label>Administrace uživatelů</label>
			<route>userRights</route>
			<resource>user</resource>
			<privilege>rights</privilege>
		</userAdmin>
		<utility>
			<label>Nástroje pro superadmina</label>
			<route>utility</route>
			<resource>utility</resource>
			<privilege>index</privilege>
			<pages>
				<clientimport>
					<label>Import klientů</label>
					<route>clientimport</route>
					<resource>utility</resource>
					<privilege>clientimport</privilege>
				</clientimport>
			</pages>
		</utility>
		<forms>
			<label>Formuláře auditů</label>
			<uri>/audit/form/index</uri>
			<resource>audit:form</resource>
			<priviledge>post</priviledge>
		</forms>
	</nav>
</configdata>