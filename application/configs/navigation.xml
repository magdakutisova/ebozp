<?xml version="1.0" encoding="UTF-8"?>
<configdata>
	<nav>
        <cantlogin>
            <label>Nemohu se přihlásit</label>
            <target>_blank</target>
            <route>cant-login</route>
        </cantlogin>
		<contacts>
			<label>Kontakty</label>
            <target>_blank</target>
			<route>contacts</route>
		</contacts>
        <about>
			<label>O GUARDIANU</label>
            <target>_blank</target>
			<route>about</route>
		</about>
		<client>
			<label>Klienti</label>
			<route>clientList</route>
			<resource>client</resource>
			<privilege>list</privilege>
			<pages>
				<choose>
					<label>Vyber ze seznamu</label>
					<route>clientList</route>
					<resource>client</resource>
					<privilege>list</privilege>
				</choose>
				<search>
					<label>Hledej</label>
					<route>search</route>
					<resource>search</resource>
				</search>
				<new>
					<label>Založ nového klienta</label>
					<route>clientNew</route>
					<resource>client</resource>
					<privilege>new</privilege>
				</new>
				<audit><label>Nový audit</label>
					<resource>audit:audit</resource>
					<privilege>create</privilege>
					<uri>/audit/audit/index</uri>
					</audit>
				</pages>
		</client>
		<clientArchivelist>
			<label>Archiv klientů</label>
			<route>clientArchivelist</route>
			<resource>client</resource>
			<privilege>archivelist</privilege>
		</clientArchivelist>
		<userAdmin>
			<label>Administrace uživatelů</label>
			<route>userRights</route>
			<resource>user</resource>
			<privilege>rights</privilege>
		</userAdmin>
		<utility>
			<label>Nástroje pro superadmina</label>
			<route>utility</route>
			<resource>utility</resource>
			<privilege>index</privilege>
			<pages>
				<clientimport>
					<label>Import klientů</label>
					<route>clientimport</route>
					<resource>utility</resource>
					<privilege>clientimport</privilege>
				</clientimport>
			</pages>
		</utility>
		<orders>
			<label>Objednávky</label>
			<uri>/audit/order/index</uri>
			<resource>audit:order</resource>
			<priviledge>index</priviledge>
		</orders>
		<diary>
			<label>Zprávy z deníku</label>
			<uri>/diary/index</uri>
			<resource>diary</resource>
			<priviledge>index</priviledge>
		</diary>
		<forms>
			<label>Formuláře auditů</label>
			<uri>/audit/form/index</uri>
			<resource>audit:form</resource>
			<priviledge>post</priviledge>
		</forms>
		<mistakecategories>
			<label>Kategorie neshod</label>
			<uri>/audit/category/index</uri>
			<resource>audit:category</resource>
			<priviledge>post</priviledge>
		</mistakecategories>
		<deadlinecategories>
			<label>Kategorie lhůt</label>
			<uri>/deadline/category/index</uri>
			<resource>deadline:category</resource>
			<priviledge>post</priviledge>
		</deadlinecategories>
	</nav>
</configdata>