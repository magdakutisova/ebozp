<?php
$subsidiaryId = Zend_Controller_Front::getInstance()->getRequest()->getParam("subsidiaryId");
$redirector = new Zend_Controller_Action_Helper_Redirector();
$url = $this->url(array("clientId" => $this->clientId, "subsidiaryId" => $this->subsidiaryId, "TYPE" => $this->REQ_TYPE), "document-documentation-index");

$redirector->gotoUrlAndExit($url);