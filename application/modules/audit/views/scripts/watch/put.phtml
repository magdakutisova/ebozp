<?php
$redirector = new Zend_Controller_Action_Helper_Redirector();
$watch = $this->watch;

$params = array(
		"watchId" => $watch->id,
		"clientId" => $watch->client_id,
		"subsidiaryId" => $watch->subsidiary_id
);

$url = $this->url($params, "audit-watch-edit");

$redirector->gotoUrlAndExit($url);